{
  "manifest_version": 3,
  "name": "Screenshot Background Editor",
  "version": "1.0",
  "description": "Add background colors to your screenshots with drag and drop functionality",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Screenshot Background Editor"
  },
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
